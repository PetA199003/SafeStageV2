else
  export NODE_PATH="/Users/pastor/Documents/SafeStage_CH/node_modules/.pnpm/@expo+cli@54.0.23_expo-router@6.0.23_expo@54.0.33_react-native@0.81.5_@babel+core@7.29._daef1aa59357a8cccc6b98050d411315/node_modules/@expo/cli/build/bin/node_modules:/Users/pastor/Documents/SafeStage_CH/node_modules/.pnpm/@expo+cli@54.0.23_expo-router@6.0.23_expo@54.0.33_react-native@0.81.5_@babel+core@7.29._daef1aa59357a8cccc6b98050d411315/node_modules/@expo/cli/build/node_modules:/Users/pastor/Documents/SafeStage_CH/node_modules/.pnpm/@expo+cli@54.0.23_expo-router@6.0.23_expo@54.0.33_react-native@0.81.5_@babel+core@7.29._daef1aa59357a8cccc6b98050d411315/node_modules/@expo/cli/node_modules:/Users/pastor/Documents/SafeStage_CH/node_modules/.pnpm/@expo+cli@54.0.23_expo-router@6.0.23_expo@54.0.33_react-native@0.81.5_@babel+core@7.29._daef1aa59357a8cccc6b98050d411315/node_modules/@expo/node_modules:/Users/pastor/Documents/SafeStage_CH/node_modules/.pnpm/@expo+cli@54.0.23_expo-router@6.0.23_expo@54.0.33_react-native@0.81.5_@babel+core@7.29._daef1aa59357a8cccc6b98050d411315/node_modules:/Users/pastor/Documents/SafeStage_CH/node_modules/.pnpm/node_modules:$NODE_PATH"
